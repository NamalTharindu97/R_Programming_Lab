function(z){
q1<- quantile(z)[2]
q3<- quantile(z)[4]
iqr<- q3 - q1
ub <- q3 + 1.5*iqr
lb <- q1 - 1.5*iqr
print(paste("upper Bound = " , ub))
print(paste("lower Bound ", lb))
print(paste("outliers :" , paste(sort(z[z<lb | z>ub]), collapse = ",")))
}
